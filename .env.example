# ============================================
# BotHunter VK Parser - Настройки
# ============================================

# -----------------
# Основные параметры
# -----------------

# Режим браузера (true = без GUI, false = с GUI для отладки)
HEADLESS=false

# Максимальное количество страниц для парсинга (оставьте пустым для парсинга всех)
MAX_PAGES=

# Путь к папке для сохранения сессии браузера
SESSION_PATH=./browser-session

# Файл для сохранения результатов
OUTPUT_FILE=results.json

# -----------------
# Дополнительные настройки
# -----------------

# Задержка между страницами в мс (опционально)
PAGE_DELAY=2000

# Таймаут для ожидания элементов в мс (опционально)
TIMEOUT=30000
